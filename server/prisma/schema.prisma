generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  name     String
  password String
  profile  Profile?
}

model Profile {
  id          Int     @id @default(autoincrement())
  foto        Bytes?
  bio         String?
  emailString String  @unique
  email       User    @relation(fields: [emailString], references: [email])
}

model Post {
  id      Int     @id @default(autoincrement())
  imagem  Bytes
  comment String?
}
